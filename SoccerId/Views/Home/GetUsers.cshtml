@model IEnumerable<SoccerId.Entities.User>

    @using Microsoft.AspNet.Identity

    @{
    ViewBag.Title = "";
    }

    <div>
        <h2>Данные пользователей (@Html.ActionLink("Метод Edit пока не подключен", "Create"))</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Имя</th>
                <th>Email</th>
            </tr>
            @if (Model.Count() == 0) {
            <tr>
                <td colspan="3" class="text-center">Нет ни одной учетной записи</td>
            </tr>
            } else {
            foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>
                   



                    <!--@Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.Id })-->
                </td>
            </tr>
            }
            }
        </table>
    </div>
    <!--@Html.ActionLink("Создать", "Create", null, new { @class = "btn btn-primary" })-->
